{
  "name": "CHANGE THIS : Extension boilerplate",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created with the awesome 4brains",
  "homepage_url": "http://vk.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "browser action demo"
  },
  "permissions": [
    "http://vk.com/*",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://vk.com/*"
      ],
      "js": [
        "js/jquery-1.11.0.min.js",
        "js/promise-1.0.0.min.js",
        "js/realsense.js",
        "js/realsenseinfo.js",
        "js/jquery.scrollTo.js",
        "src/inject/inject.js",
        "src/inject/api.js"
      ]
    }
  ]
}